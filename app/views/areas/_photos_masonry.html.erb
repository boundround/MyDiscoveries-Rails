<div id="photos-masonry">
  <p>HI</p>
  <% if @area.photos.length > 0 %>
    <% @area.photos.each_with_index do |photo, index| %>
        <div class="photo"><img sizes="(min-width: 40em) 80vw, 100vw" class="place-photo" src="<%= photo.path_url(:small) %>" srcset="<%= photo.path_url(:small) %> 450w, <%= photo.path_url(:medium) %> 640w, <%= photo.path_url(:large) %> 960w" />
        </div>
    <% end %>
  <% end %>
</div>

<script>
  // Picture element HTML5 shiv
  document.createElement( "picture" );



</script>
<script src="/assets/js/picturefill.js" async></script>
