<div class="photo-container">
  <h2>Add New Game</h2>
  <%= simple_form_for Game.new do |f| %>
    <div class="form-group">
      <%= f.input :url, label: "Enter Game URL", input_html: { class: 'form-control'}  %>
      <%= f.input :place_id, as: :hidden, input_html: {value: @place.id } %>
    </div>
    <div class="form-group">
      <%= f.button :submit%>
    </div>
</div>
  <% end %>
