<% if user_signed_in? %>
  <% if current_user.try(:admin) || current_user.has_role?("publisher") || current_user.has_role?("editor") || current_user.has_role?("contributor") %>
    <nav class="navbar navbar-default" role="navigation">
      <div class="container-fluid">
        <div class="collapse navbar-collapse">
          <ul class='nav navbar-nav'>
            <li><%= link_to 'Home', root_path %></li>
            <li><%= link_to 'All Countries', countries_path %></li>
            <li><%= link_to 'New Country', new_country_path %></li>
            <li><%= link_to 'All Areas', areas_path %></li>
            <li><%= link_to 'New Area', new_area_path %></li>
            <li><%= link_to 'All Places', places_path %></li>
            <li><%= link_to 'Assets In Review', "/places/all_edited" %></li>
            <li><%= link_to 'Publishing Queue', "/places/publishing_queue" %></li>
            <li><%= link_to 'New Place', new_place_path %></li>
            <li><%= link_to 'All Programs', programs_path %></li>
            <li><%= link_to 'New Program', new_program_path %></li>
            <li><%= link_to 'Edit Place Tags', "/places/tags" %></li>
            <li><%= link_to 'Edit Program Tags', "/programs/tags" %></li>
            <li><%= link_to 'User Content Review', "/users/draft_content" %></li>
            <li><%= link_to 'User Created Places', "/places/user_created" %></li>
            <% if current_user.try(:admin) %>
              <li><%= link_to 'Show/Edit Users', users_path %></li>
              <li><%= link_to 'Rails Admin', rails_admin_path %></li>
            <% end %>
            <% if user_signed_in? %>
              <li><%= link_to 'Sign Out', destroy_user_session_path, method: :delete %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </nav>
  <% end %>
<% end %>

