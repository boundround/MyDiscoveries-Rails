    <%= javascript_include_tag 'froogaloop' %>



		<div id="place-holder"></div>
	  <%= render "/layouts/newsletter_sign_up" %>
  	  <% flash.each do |type, message| %>
        <% if !bootstrap_class_for(type).eql?("timedout")%>
		  <% if is_flash_key_for_tracking?(type) %>
            <div class="hidden <%= type %>"></div>
          <% else %>
              <div class="alert <%= bootstrap_class_for(type) %> fade in">
    		    <button class="close" data-dismiss="alert">Ã—</button>
    		    <%= message %>
    		  </div>
          <% end %>
         <% end %>
  		<% end %>

		<div class="alert-wrapper">
            <div class="alert alert-info want-alert" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
              Thanks! <strong>We're on it!</strong>
            </div>
        </div>

		<%= render "/layouts/admin_menu" %>

	  <div class="app-wrap">
	    <%= yield %>
	  </div>

	  <%= render "/layouts/signin_modals" unless user_signed_in? %>
	  <%= render "/layouts/share_modal" %>

	</body>



