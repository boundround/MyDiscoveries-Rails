        <div class="br15_top_search container-fluid">
            <div class="br15_search">
                <div class="input-group">
                    <input type="text" id="search-box" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                        <button class="btn btn-default" id="magnifying-glass" type="button"><span class="fa fa-search"></span></button>
                    </span>
                </div>
            </div>
            <div class="br15_search_result">
              <%= render "search_suggestions/search_results" %>
            </div>
              <%= render 'layouts/profile_dropdown' %>
            <div class="row br15_header">
                <div class="col-xs-12 text-center">
                    <a href="#" class="center-block br15_logo"><img src="<%= asset_path('br_logo_new.png') %>" class="img-responsive center-block home-logo" alt=""></a>
                    <h3 class="text-center text-uppercase br15_heading_main">Sign UP For Bound Round And Get Access to Amazing features!</h3>
                </div>
            </div>
            <div class="row br15_map br15_collapse hidden-xs">
                <div class="br15_map_overlay"></div>
                <a href="#" class="btn btn-default br15_Slide_Up_BTN"><span class="fa fa-globe"></span> <span class="fa fa-chevron-up"></span></a>
                    <div id="br15_map">

                    </div>
            </div>
        </div>
        <div class="br15_videos container-fluid">
            <a href="<%= videos_path %>" class="btn btn-default br15_Title_See_All">videos <span>See All</span></a>
            <div class="row">
                <div id="br15_owl_video" class="owl-carousel">
                  <% @videos.each do |video| %>
                      <div><!-- 16:9 aspect ratio -->
                          <div class="embed-responsive embed-responsive-16by9">
                              <iframe class="embed-reponsive-item" src="//player.vimeo.com/video/<%= video.vimeo_id %>?api=1" frameborder="2" data-title="<%= video.title %>" data-description="<%= video.description %>" webkitallowfullscreen mozallowfullscreen allowfullscreen>
                                          </iframe>
                              <!-- <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/51_gSa1OdEE"></iframe> -->
                          </div>
                      </div>
                    <% end %>
                </div>
            </div>
        </div>
        <div class="br15_videos br15_photos_cont container-fluid">
            <a href="#" class="btn btn-default br15_Title_See_All">Photos</a>
            <div class="row">
                <div id="br15_owl_photos" class="owl-carousel">
                  <% @photos.each do |photo| %>
                    <div><!-- 16:9 aspect ratio -->
                      <a href="<%= place_path(photo.place.id) %>">
                        <div class="embed-responsive embed-responsive-16by9">
                            <img class="embed-responsive-item" src="<%= photo.path_url(:medium) %>">
                        </div>
                      </a>
                    </div>
                  <% end %>
                </div>
            </div>
        </div>
        <div class="br15_features container-fluid">
            <div class="row">
                <div class="col-sm-6">
                    <div class="row br15_instagram">
                        <div class="col-xs-12">
                            <a href="#" class="btn btn-default br15_features_btn br15_Title_See_All">Instagram</a>
                            <div class="br15_row_mob">
                              <div id="instafeed"></div>
                                <ul class="list-unstyled">
                                    <li>
                                        <img src="" class="img-responsive" alt="">
                                    </li>
                                    <li class="clearfix"></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="row br15_leaderboard">
                        <div class="col-xs-12">
                            <a href="#" class="btn btn-default br15_features_btn br15_features_btn_leader br15_Title_See_All">Leaderboard</a>
                            <ul class="list-unstyled br_Leaderboard">
                              <% @leaderboard.each_with_index do |leader, index| %>
                                <li>
                                  <a href="">
                                  <span class="br_avtar">
                                    <% if leader.user.avatar.blank? %>
                                      <img src="<%= asset_path('Badge.png') %>" alt="">
                                    <% else %>
                                      <img src="<%= leader.user.avatar_url %>" alt="">
                                    <% end %>
                                      <span>
                                          <%= leader.balance %>
                                      </span>
                                  </span>
                                  <span class="br_avtar_no">
                                      <%= (index + 1).to_s %>
                                  </span>
                                  <span class="br_user_name"><%= strip_domain_from_username(leader.user) %></span>
                                  </a>
                                </li>
                              <% end %>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="br15_features container-fluid">
            <div class="row">
                <div class="col-sm-6">
                    <div class="row br15_reviews">
                        <div class="col-xs-12">
                            <a href="#" class="btn btn-default br15_features_btn br15_Title_See_All">Reviews</a>
                            <hr class="br15_hr"/>
                            <ul class="list-unstyled">
                              <% @reviews.each do |review| %>
                                <li>
                                    <a href="<%= place_path(review.reviewable) + "#reviews" %>" class="row center-block">
                                      <span class="col-xs-6 center-block">
                                          <span class="row center-block">
                                              <%= !review.title.blank? ? review.title : review.reviewable.display_name %>
                                          </span>
                                      </span>
                                      <span class="col-xs-6 center-block text-right">
                                          <span class="row center-block">
                                            <% if review.reviewable.quality_average %>
                                              <% review.reviewable.quality_average.avg.round.times do %>
                                                <span class="fa fa-star"></span>
                                              <% end %>
                                            <% end %>
                                          </span>
                                      </span>
                                    </a>
                                </li>
                              <% end %>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="row br15_stories br15_reviews">
                        <div class="col-xs-12">
                            <a href="#" class="btn btn-default br15_features_btn  br15_Title_See_All">Stories</a>
                            <hr class="br15_hr"/>
                            <ul class="list-unstyled ">
                              <% @stories.each do |story| %>
                                <li>
                                    <a href="<%= place_path(story.storiable) + "#stories" %>" class="row center-block">
                                        <%= story.title.blank? ? story.storiable.display_name : story.title %>
                                    </a>
                                </li>
                              <% end %>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer class="container-fluid text-center br15_footer">
