<div class+"container">
  <div id="table-container">
    <h3>Places with assets in review</h3>
    <table class="table table-striped" id="video-table">
      <thead>
        <tr>
          <th>Place</th>
          <th>Country</th>
          <th>Thumbnail</th>
          <th>Content Type</th>
          <th>Content ID</th>
        </tr>
      </thead>
      <tbody>
        <% @all_content.each do |content| %>
            <tr>
              <td><%= link_to content.photoable.display_name, edit_place_path(content.photoable) %></td>
              <td><%= content.photoable.country.display_name if content.photoable && content.photoable.country %></td>
              <td><img src="<%= thumbnail_for(content) %>" class="photo-edit-thumb"></td>
              <td><%= content.class %></td>
              <td><%= content.id %></td>
            </tr>
          <% end %>
      </tbody>
    </table>
  </div>
</div>
