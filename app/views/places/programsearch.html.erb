		<script>
		function get_search_params()
		{
			return "term="+$("#search_input").val();
		}


		function ss_goto_search()
		{
			window.location.href = encodeURI("/places/programsearch?"+get_search_params());
		}

		function ss_goto_list_results()
		{
			window.location.href = encodeURI("/places/programsearchresultslist?"+get_search_params());
		}

		function ss_goto_map_results()
		{
			window.location.href = encodeURI("/places/programsearchresultsmap?"+get_search_params());
		}
		</script>

    <script>
        $(document).ready(function () {

            $("#owl-demo").owlCarousel({

                autoPlay: 3000, //Set AutoPlay to 3 seconds

                items: 3,
                itemsDesktop: [1199, 3],
                itemsDesktopSmall: [979, 3],                
                itemsTablet: [767, 3],
                itemsMobile: [480, 2]

            });

