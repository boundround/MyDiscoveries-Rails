    <script type="text/javascript">
      function initialize() {
        var mapOptions = {
					<% if @zoom && !@places.nil? then %>
	          center: { lat: <%=@places[0].latitude%>, lng: <%=@places[0].longitude%>},
	           disableDefaultUI: true,
	          zoom: <%=@zoom%>,
					<% else %>
	          center: { lat: 0, lng: 0},
	           disableDefaultUI: true,
	          zoom: 8,
					<% end %>
					maxZoom: 15,
          mapTypeId: google.maps.MapTypeId.ROADMAP,
          styles:[ { "featureType": "water", "stylers": [ { "color": "#6CB7CC" } ] },{ "featureType": "landscape", "stylers": [ { "color": "#F1EEE7" } ] },{ } ]
        };
        var map = new google.maps.Map(document.getElementById('map-canvas'),
            mapOptions);
      
				var markers = [];
			<% @places.each do |place| %>
				(function (){
	        <%if place.categories[0].nil? %>
						var image ="https://d1w99recw67lvf.cloudfront.net/category_icons/sights_pin.png";
					<% else %>
						var image ="https://d1w99recw67lvf.cloudfront.net/category_icons/<%=place.categories[0].identifier%>_pin.png";
	        <% end %>

					var myLatLng = new google.maps.LatLng(<%=place.latitude%>, <%=place.longitude%>);
   
   
				   var contentString = '<a href="/places/placeprograms?id=<%=place.id%>"<div id="content">'+
					'<div id="siteNotice">'+
					'</div>'+
					//      '<h1 id="firstHeading" class="firstHeading">Uluru</h1>'+
					'<div class="popup_img" content: "<%=place.categories[0].name%>" style="background: transparent url("<%=first_category_icon(place)%>")"><img src="<%=place.photos[0].path%>"/></div>'+
					'<div id="bodyContent">'+
					'<p><b><%=place.display_name%></b> <h6><%=place.area.display_name%></h6>'+
					'</p>'+

					'</div>'+
					'<p class="popup-bottom">k-12 <span class="fa">&#xf004; &#xf004; &#xf004; <i>&#xf004; &#xf004;</i>  </span></p>' +
					'</div></a>';

					var infowindow = new google.maps.InfoWindow({
						content: contentString
					});

					var marker = new google.maps.Marker({
						position: myLatLng,
						map: map,
						title: "<%=place.display_name%>",
						icon: image
				  });
					
					markers.push(marker);

				  google.maps.event.addListener(marker, 'click', function() {
				    infowindow.open(map,marker);
				  });
				})();
			<% end %>

				var bounds = new google.maps.LatLngBounds();
				for(i=0;i<markers.length;i++) {
				 bounds.extend(markers[i].getPosition());
				}
				map.fitBounds(bounds);
 }
 google.maps.event.addDomListener(window, 'load', initialize);
    </script>


