<div id="table-container">
  <h1>All Programs</h1>

  <table class="table table-striped" id="place-table">
    <thead>
    <tr>
			<th>Place</th>
      <th>Name</th>
      <th>Subjects</th>
      <th>Yearlevels</th>
			<th>Hero Link</th>
      <th>Program Link</th>
			<th>Book Now Link</th>
    </tr>
    </thead>
    <tbody>
    <% @programs.each do |program| %>
        <tr>
          <td><%= program.place.display_name %></td>
          <td><%= link_to program.name, edit_program_path(program) %></td>
          <td><%= program.programsubject_list %></td>
          <td><%= program.programyearlevel_list %></td>
					<%if program.heroimagepath %>
						<td><div><img src="<%=program.heroimagepath%>" style="width:50px;height:50px;"/></td>
					<%else%>
						<td><%= "MISSING" %></td>
					<%end%>
					<%if program.programpath %>
          <td><%= link_to "Try me", program.programpath, :target => "_blank"%></td>
					<%else%>
						<td><%= "MISSING" %></td>
					<%end%>
					<%if program.booknowpath %>
          <td><%= link_to "Try me", program.booknowpath, :target => "_blank"%></td>
					<%else%>
						<td><%= "MISSING" %></td>
					<%end%>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>