<% if instance_variable_get("@#{params[:resources]}").present? and params[:resources].present? %>
  $("#<%= params[:resources] %>-list").append("<%= escape_javascript( render(partial:  params[:pagination_template].present?? params[:pagination_template] : "#{params[:resources]}_list", collection: instance_variable_get("@#{params[:resources]}"), as: :story )) %>");
  <% if last_page? instance_variable_get("@#{params[:resources]}") %>
    $("div#<%= params[:resources] %>-single-pagination-container").remove();
  <% else %>  
    $("div#<%= params[:resources] %>-single-pagination-container").find("a.see-more__link").attr('href', "<%= paginate_stories_region_path(@region, "#{params[:resources]}_page" => instance_variable_get("@#{params[:resources]}").next_page, resources: params[:resources], pagination_template: params[:pagination_template] )%>" )
  <% end %>

<% end %>
setupModal();