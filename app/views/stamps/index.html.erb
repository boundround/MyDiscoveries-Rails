<div class="container">
  <div class="text-left" style="position: relative; left: 20px; top: 20px;">

    <div class="br-image">
        <div class="br-header">
          <a href="https://boundround.com/" rel="nofollow"><img src="https://s3-ap-southeast-2.amazonaws.com/brwebproduction/images/br-logo-new-2016-white.png" style="width: 100px; position: absolute; top: 0; left: 0; right: 0;"></a>
       </div>
    </div>
    
    <h1 style="padding: 15px; padding-left: 140px; margin-bottom: 40px; color: white; font-weight: 100;"><%= @place.display_name %> - all stamps</h1>

    <ul>

    <% @stamps.each do |stamp| %>

      <p style="background:white; padding: 0 15px; margin: 5px 100px;">Serial: <%= stamp.serial %></p>
      <p style="background:white; padding: 0 15px; margin: 5px 100px;">Place: <%= stamp.place.display_name %></p>
      <a href="<%= edit_place_stamp_path(@place, stamp) %>">Edit Stamp</a>
      <hr>

    <% end %>

    <%= render 'form' %>



    </ul>
  </div>
</div>