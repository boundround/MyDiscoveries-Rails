<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3 ">
			<div class="col-md-12">
				<div class="outer-profile">
	              <img src="">
	            </div>
			</div>
			<div class="col-md-12">
			<table class="table table-striped">
			<% @stories.each do |story| %>
				<tr>
					<td><h3> <%= story.title.html_safe %></h3></td>
					<td><%= link_to "Show", story_path(story) %>
					<%= link_to "Edit", edit_story_place_path(story.storiable, story) %>
					<%= link_to "Delete", story_path(story), method: :delete, data: { confirm: 'Are you certain you want to delete this?' } %></td>
				</tr>
			<% end %>
			</table>
			</div>
		</div>
	</div>
</div>