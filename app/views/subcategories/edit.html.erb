<div class="container">
  <h2>Edit Subcategory</h2>
  <%= simple_form_for @subcategory do |f| %>
    <%= f.input :name, input_html: { class: 'form-control' } %>
    <%= f.input :category_type, input_html: { class: 'form-control' } %>
    <%= f.input :icon, as: :file %>
    <%= f.submit :submit %>
  <% end %>
</div>
