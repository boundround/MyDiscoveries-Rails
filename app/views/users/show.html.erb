<%= render 'user_nav' %>
        <div class="br_tab_xs_header visible-xs container-fluid">
            <div class="row">

                <div class="col-xs-12">
                    <a href="#" data-toggle="offcanvas"><span class="fa fa-bars"></span></a> &nbsp; <a href="#" class="br_logo_top_header">BoundRound</a>
                </div>
            </div>
        </div>

         <div class="container-fluid br_container_tab">
             <div class="row br_profile_header hidden-xs">
                 <div class="col-sm-8 col-sm-offset-2">
                     <div class="row">
                         <div class="col-sm-3 col-sm-offset-1 large-avatar-container">
                             <div class="br_shell col-sm-offset-3">
                              <a href="#" data-toggle="modal" data-target="#br_modal">
                                  <% if @user.avatar_url %>
                                    <img src="<%= current_user.avatar_url %>" class="img-circle img-responsive large-avatar" alt="">
                                  <% else %>
                                    <i class="fa fa-user fa-5x"></i>
                                  <% end %>
                              </a>
                            </div>
                         </div>
                         <div class="col-sm-8 br_profile_detail">
                             <div class="row">
                                 <div class="col-md-12">
                                    <div class="row">
                                        <div class="col-sm-11 col-sm-offset-1">
                                            <h2><%= @user.name %><br>
                                                <span>Member Since <%= @user.created_at.strftime("%Y") %></span>
                                            </h2>
                                        </div>
                                    </div>
                                 </div>
                             </div>
                             <!-- <div class="row">
                                 <div class="col-xs-4 br_progress_bar text-center">
                                    <div class="br_circle_cont">
                                        <div id="circle"></div>
                                        <p>KID</p>
                                    </div>
                                 </div>
                                 <div class="col-xs-4 br_progress_bar text-center">
                                     <div class="br_circle_cont">
                                         <div id="circle2"></div>
                                         <p>REPORTER</p>
                                     </div>
                                 </div>
                                 <div class="col-xs-4">
                                     <div class="br_circle_cont text-center">
                                         <div id="circle3" > </div>
                                         <p>BOARD MEMBER</p>
                                     </div>
                                 </div>
                             </div> -->
                         </div>
                     </div>
                 </div>
             </div>
             <div class="row br_tabs">
                     <!-- Nav tabs -->
                     <div class="col-sm-4">
                         <ul class="nav row br_no_row nav-tabs" role="tablist">
                             <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Summary</a></li>
                             <!-- <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Transactions</a></li> -->
                         </ul>
                     </div>
                 </div>
             <div class="row">
                     <!-- Tab panes -->
                    <div class="col-sm-12">
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="home">
                                <div class="row">
                                    <div class="col-sm-4 ">
                                        <div class="row br_no_row br_sidebar">
                                            <div class="col-sm-12">
                                                <h1>
                                                  <% if @user.points_balance %>
                                                      <%= @user.points_balance.balance.to_s %>
                                                    <% else %>
                                                      0
                                                    <% end %>
                                                    <br><span>Total Points</span>
                                                </h1>
                                                <h3>Leaderboard</h3>
                                                <ul class="list-unstyled br_Leaderboard">
                                                  <% @leaderboard.each_with_index do |leader, index| %>
                                                    <li>
                                                      <a href="">
                                                      <span class="br_avtar">
                                                        <% if leader.user.avatar.blank? %>
                                                          <img src="<%= asset_path('Badge.png') %>" alt="">
                                                        <% else %>
                                                          <img src="<%= leader.user.avatar_url %>" alt="">
                                                        <% end %>
                                                          <span>
                                                              <%= leader.balance %>
                                                          </span>
                                                      </span>
                                                      <span class="br_avtar_no">
                                                          <%= (index + 1).to_s %>
                                                      </span>
                                                      <span class="br_user_name"><%= strip_domain_from_username(leader.user) %></span>
                                                      </a>
                                                    </li>
                                                    <% end %>
                                                </ul>
                                               <!-- <div class="row visible-xs">
                                                   <a href="#" class="btn btn-block btn-default br_load_more_side">Load More</a>
                                               </div> -->
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-sm-8">
                                        <div class="row hidden-xs">
                                            <div class="col-sm-6">
                                              <style>
                                                #br_user_photos {
                                                  <% unless @user.user_photos.blank? %>
                                                    background: url("<%= @user.user_photos[rand(@user.user_photos.length - 1)].path_url(:small) %>") no-repeat;
                                                  <% end %>
                                                  background-size: cover;
                                                }
                                              </style>
                                                <div class="row br_user_activity br_bor_top_bot" id="br_user_photos">
                                                    <a href="/users/photos" class="br_ellipse"><%= @user.user_photos.active.length %><br/><span>Photos</span></a>
                                                    <a href="#" class="br_point_earn">Add New<br/><span><%= calculate_user_points_for_asset(@user.user_photos.active) %>PTS</span></a>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                              <style>
                                                #br_user_stories {
                                                  <% unless @user.user_photos.blank? %>
                                                    background: url("<%= @user.user_photos[rand(@user.user_photos.length - 1)].path_url(:small) %>") no-repeat;
                                                  <% end %>
                                                  background-size: cover;
                                                }
                                              </style>
                                               <div class="row br_user_activity br_user_upload br_bor_all" id="br_user_stories">
                                                   <a href="/users/stories" class="br_ellipse"><%= @user.stories.active.length %><br/><span>Stories</span></a>
                                                   <a href="#" class="br_point_earn">Add New<br/><span><%= calculate_user_points_for_asset(@user.stories.active) %>PTS</span></a>
                                               </div>
                                            </div>
                                        </div>
                                        <div class="row hidden-xs">
                                            <div class="col-sm-6">
                                              <style>
                                                #br_user_reviews {
                                                  <% unless @user.user_photos.blank? %>
                                                    background: url("<%= @user.user_photos[rand(@user.user_photos.length - 1)].path_url(:small) %>") no-repeat;
                                                  <% end %>
                                                  background-size: cover;
                                                }
                                              </style>
                                                <div class="row br_user_activity" id="br_user_reviews">
                                                    <a href="/users/reviews" class="br_ellipse"><%= @user.reviews.active.length %><br/><span>Reviews</span></a>
                                                    <a href="#" class="br_point_earn">Add New<br/><span><%= calculate_user_points_for_asset(@user.reviews.active) %>PTS</span></a>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="row br_user_activity br_bg_purple br_bor_all">
                                                    <h1><a href="#">Refer Friends Get Points</a></h1>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row visible-xs">
                                            <div class="col-xs-12">
                                                <div id="owl-example" class="owl-carousel">
                                                    <div>
                                                        <div class="col-sm-12">
                                                          <style>
                                                            #br_user_mobile_photos {
                                                              <% unless @user.user_photos.blank? %>
                                                                background: url("<%= @user.user_photos[rand(@user.user_photos.length - 1)].path_url(:small) %>") no-repeat;
                                                              <% end %>
                                                              background-size: cover;
                                                            }
                                                          </style>
                                                          <div class="row br_user_activity" id="br_user_mobile_photos">
                                                              <a href="/users/photos" class="br_ellipse"><%= @user.user_photos.active.length %><br/><span>Photos</span></a>
                                                              <a href="#" class="br_point_earn">Add New<br/><span><%= calculate_user_points_for_asset(@user.user_photos.active) %>PTS</span></a>
                                                          </div>
                                                        </div>
                                                    </div>
                                                    <div>
                                                      <div class="col-sm-12">
                                                        <style>
                                                          #br_user_mobile_stories {
                                                            <% unless @user.user_photos.blank? %>
                                                              background: url("<%= @user.user_photos[rand(@user.user_photos.length - 1)].path_url(:small) %>") no-repeat;
                                                            <% end %>
                                                            background-size: cover;
                                                          }
                                                        </style>
                                                      <div class="row br_user_activity br_user_upload" id="br_user_mobile_stories">
                                                          <a href="/users/stories" class="br_ellipse"><%= @user.stories.active.length %><br/><span>Stories</span></a>
                                                           <a href="#" class="br_point_earn">Add New<br/><span><%= calculate_user_points_for_asset(@user.stories.active) %>PTS</span></a>
                                                      </div>
                                                    </div>
                                                    </div>
                                                    <div>
                                                        <div class="col-sm-12">
                                                          <style>
                                                            #br_user_mobile_reviews {
                                                              <% unless @user.user_photos.blank? %>
                                                                background: url("<%= @user.user_photos[rand(@user.user_photos.length - 1)].path_url(:small) %>") no-repeat;
                                                              <% end %>
                                                              background-size: cover;
                                                            }
                                                          </style>
                                                          <div class="row br_user_activity" id="br_user_mobile_review">
                                                              <a href="/users/reviews" class="br_ellipse"><%= @user.reviews.active.length %><br/><span>Reviews</span></a>
                                                              <a href="#" class="br_point_earn">Add New<br/><span><%= calculate_user_points_for_asset(@user.reviews.active) %>PTS</span></a>
                                                          </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-12">
                                                        <div class="row br_user_activity br_bg_purple ">
                                                            <h1><a href="#">Refer Friends Get Points</a></h1>
                                                        </div>
                                                    </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- <div role="tabpanel" class="tab-pane" id="profile"><h1>Pending</h1></div> -->
                        </div>
                    </div>
             </div>
         </div>
